{
    "name": "bolden/hello-translated",
    "description": "Greet your website visitors in their native language!",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "translate"
    ],
    "support": {
        "docs": "https://github.com/boldenamsterdam/hellotranslated/blob/master/README.md",
        "issues": "https://github.com/boldenamsterdam/hellotranslated/issues"
    },
    "license": "MIT",
    "authors": [{
        "name": "Bolden B.V.",
        "homepage": "https://www.bolden.nl"
    }],
    "require": {
        "craftcms/cms": "^3.0.0",
        "peterkahl/country-to-locale": "@dev"
    },
    "autoload": {
        "psr-4": {
            "bolden\\hellotranslated\\": "src/"
        }
    },
    "extra": {
        "name": "Hello translated",
        "handle": "hello-translated",
        "schemaVersion": "1.0.0",
        "hasCpSettings": false,
        "hasCpSection": false,
        "class": "bolden\\hellotranslated\\HelloTranslated"
    }
}
